<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiFilePickerSample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiFilePickerSample"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiFilePickerSample">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
    Title="FilePicker"
    ContentTemplate="{DataTemplate local:MauiFilePickerPage}"
    Route="MauiFilePickerPage" />

</Shell>
