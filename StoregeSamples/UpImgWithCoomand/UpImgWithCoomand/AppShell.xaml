<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UpImgWithCoomand.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:UpImgWithCoomand"
    xmlns:view ="clr-namespace:UpImgWithCoomand.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="UpImgWithCoomand">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="From Image"
        ContentTemplate="{DataTemplate view:FromTakeImage}"
        Route="FromTakeImage" />

    <ShellContent
        Title="From Folder"
        ContentTemplate="{DataTemplate view:FromFolder}"
        Route="FromFolder" />

</Shell>
